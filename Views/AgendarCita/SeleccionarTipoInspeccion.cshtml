@model IEnumerable<dynamic>

<select id="tipoInspeccionSelect" class="form-control">
    <option value="">Seleccionar Tipo de Inspección</option>
    @foreach (var tipo in Model)
    {
        <option value="@tipo.PkTipoInspeccion">@tipo.Titulo</option>
    }
</select>

<button type="button" class="bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded mt-4" data-bs-dismiss="modal">Seleccionar</button>
